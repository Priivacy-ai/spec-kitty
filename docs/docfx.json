{
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "toc.yml",
          "1x/*.md",
          "1x/toc.yml",
          "2x/*.md",
          "2x/toc.yml"
        ],
        "exclude": [
          "**/_*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/spec-kitty"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Spec Kitty Documentation (1.x and 2.x)",
      "_appName": "Spec Kitty",
      "_appFooter": "Spec Kitty documentation tracks for 1.x and 2.x",
      "_enableSearch": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/Priivacy-ai/spec-kitty",
        "branch": "2.x"
      },
      "_appLogoPath": "assets/images/logo_small.webp",
      "_appFaviconPath": "assets/images/logo_small.webp"
    }
  }
}
